{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".next/**", "!.next/cache/**"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"test": {
			"dependsOn": ["build"],
			"outputs": []
		},
		"test:e2e": {
			"dependsOn": ["^build"],
			"cache": false
		},
		"lint": {
			"outputs": []
		}
	}
}
